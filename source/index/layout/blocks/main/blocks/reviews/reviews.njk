<section class="review">
    <div class="review__container-top">
        <div class="review__title">Отзывы</div>
        <div class="review__video-wrapper">
            <div class="review__video-items">
                <button class="review__button button--prev" id="#button-prev">
                    <img src="../../../../images/arrow.svg" alt="">
                </button>
                <div class="review__video-container">
                    <div class="review__video active" id="video_1">
                        <video width="100%" height="300px" controls="controls">
                            <source src="{{ path }}video/1.mp4" type="video/mp4">
                        </video>
                        {# <img src="../../../../images/photo_1.png" alt=""> #}
                    </div>
                    <div class="review__video" id="video_2">
                        <video width="100%" height="300px" controls="controls">
                            <source src="{{ path }}video/2.mp4" type="video/mp4">
                        </video>
                        {# <img src="../../../../images/photo_2.png" alt=""> #}
                    </div>
                    <div class="review__video" id="video_3">
                        <video width="100%" height="300px" controls="controls">
                            <source src="{{ path }}video/3.mp4" type="video/mp4">
                        </video>
                        {# <img src="../../../../images/bg.png" alt=""> #}
                    </div>
                </div>
                <button class="review__button review__button--next" id="#button-next">
                    <img src="../../../../images/arrow.svg" alt="">
                </button>
            </div>
            <div class="review__video-buttons">
                <button class="review__button-circle active" data-button="#video_1"></button>
                <button class="review__button-circle" data-button="#video_2"></button>
                <button class="review__button-circle" data-button="#video_3"></button>
            </div>
